<Window x:Class="boop.wind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Car"
        mc:Ignorable="d"
        Title="wind" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://i.pinimg.com/originals/55/66/36/556636f3bfe8906cf7b9a06c520c8c0c.jpg"/>
        </Grid.Background>
        <Image x:Name="img" HorizontalAlignment="Left" Height="189" Margin="54,47,0,0" VerticalAlignment="Top" Width="317"/>
        <TextBox x:Name="txtMil" HorizontalAlignment="Left" Height="28" Margin="109,292,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="162"/>
        <Label Content="Mileage" HorizontalAlignment="Left" Margin="107,253,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="15" Foreground="#FFE4EFF1"/>

    </Grid>
</Window>
